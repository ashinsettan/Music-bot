# API ID and API Hash from my.telegram.org
# Required
API_ID=''
API_HASH=''

# Pyrogram session string from @genstr_robot
# Required
SESSION=''

# Telegram Bot Token from @BotFather
# Optional
BOT_TOKEN=''

# IDs of users with sudo permissions, separated by spaces
# Optional
SUDOERS=''

# Command prefixes for bot commands, separated by spaces
# Optional
PREFIX='!'

# Default language for bot responses in groups
# Optional
LANGUAGE='en'

# Custom stream quality for playback in groups (e.g., low, medium, high)
# Optional
QUALITY='high'

# Default stream mode for playback in groups ('audio' or 'video')
# Optional
STREAM_MODE='audio'

# Restrict non-admin users from playing media in groups
# Optional (set to 'True' to restrict, 'False' to allow all)
ADMINS_ONLY='False'

# Spotify credentials for playing Spotify links or playlists
# Optional
SPOTIFY_CLIENT_ID=''
SPOTIFY_CLIENT_SECRET=''
